/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or (a=a, b=b, out=s1);
    Nand (a=a, b=b, out=s2);
    And (a=s1, b=s2, out=out);
}